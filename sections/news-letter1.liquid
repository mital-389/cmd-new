{% schema %}
  {
    "name": "News Latter",
    "settings": [
      {
        "type":"text",
        "id":"heading",
        "label":"Heading",
        "default":"Connect With Us"
      },
      {
        "type":"textarea",
        "id":"paragraph",
        "label":"Text",
        "default":"Stay up to date with the latest news on new arrivals, exclusive sales, event invites, and more."
      },
      {
        "type":"text",
        "id":"placeholder",
        "label":"Placeholder",
        "default":"Subscribe to Newsletter"
      },
      {
        "type":"text",
        "id":"svgbtn",
        "label":"Svg Button"
      }
	],
	"blocks":[
		{
			"type":"news-letter",
			"name":"News Letter",
			"settings":[
				{
					"type":"image_picker",
					"id":"image",
					"label":"select Image"
				},
				{
					"type":"text",
					"id":"svg",
					"label":"Svg Image"
				},
				{
					"type":"text",
					"id":"svg-details",
					"label":"Image Description",
					"default":"@fortressofinca"
				}
			]
		},
		{
			"type":"social",
			"name":"Social Media",
			"settings":[
				{
					"type":"text",
					"id":"social",
					"label":"Social Media Image"
				},
				{
					"type":"text",
					"id":"sociallink",
					"label":"Social Media Link"
				}
			]
		}
	],
	"presets":[
		{
          "name":"News Letter",
          "category":"news letter"
		}
	]
  }
{% endschema %}