                </div>
            </div>
        
            <div class="col-two">
                <div class="subscribe-area">
                    <div class="sec-title">
                        <h2>{{ section.settings.heading }}</h2>
                        <p>{{ section.settings.paragraph }}</p>
                    </div>
                    <div class="subscribe-form">
                        <form>
                            <input type="email" placeholder="{{ section.settings.placeholder }}" class="newsletter-field">
                            <button class="nl-btn">{{ section.settings.svgbtn }}</button>
                        </form>
                    </div>
                    <div class="social-media">
                        <ul>
                          	{%- for block1 in section.blocks -%}
                              {%- if block1.type=='social' -%}
                              <li>
                                 {%- if block1.settings.sociallink != blank -%}<a href="{{block1.settings.sociallink}}">{{ block1.settings.social }}</a>{%- endif -%}
                              </li>
                              {%- endif -%}
                          	{%- endfor -%}
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
